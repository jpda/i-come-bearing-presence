{
  "proxies": {
    "permanent redirect": {
      "matchCondition": {
        "methods": [
          "GET"
        ],
        "route": "/"
      },
      "responseOverrides": {
        "response.statusCode": "301",
        "response.headers.Location": "https://presence.jpda.app/auth/start"
      }
    }
  }
}